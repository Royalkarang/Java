public class MaxArray {
    static void maximum(int arr[]){
        int max=arr[0];
        for(int i=0;i<arr.length;i++){
                if(max < arr[i]){
                    max=arr[i];
                }
            }
        System.out.println(max);
    }
    public static void main(String[] args) {
        int arr[]= {3,5,6,7,8,88,99};
        maximum(arr);
    }
}
